[user]
  email = frajaquico@yahoo.es
  name  = Francisco Quintero
[alias]
  st   = status
  a    = add
  aA   = add -A
  cm   = commit
  cma  = commit -am
  b    = branch
  bd   = branch -d
  cob  = checkout -b
  co   = checkout
  mrg  = merge
  rbs  = rebase
  ps   = push
  pl   = pull
  f    = fetch
  fups = fetch upstream
  pr   = pull-request
  rso  = remote show origin
  rsup = remote show upstream
  pruneorigin = remote prune origin
  l    = log
  lol  = log --oneline
  lg   = log --graph
  stashl = stash list
  stashp = stash pop
[core]
  editor = nano
[push]
  # See details about this config here https://git-scm.com/docs/git-config/2.20.0#git-config-pushdefault
  default = current
