# Shell
alias l="ls"
alias ll="ls -l"
alias la="ls -a"
alias ..="cd .."
alias ...="cd ../.."
alias psg="ps aux | grep"
alias h="history"
alias reload="source ~/.profile"
alias os_version="lsb_release -d"
alias 64bit="getconf LONG_BIT"
alias reclear="reload && clear"
alias loadgithub="ssh-add ~/.ssh/gh_iw"

# Folders
alias projects="cd ~/projects"
alias dotfiles="cd ~/projects/dotfiles"

# Luna Projects
alias lunaforms="cd ~/projects/luna-project/patient-forms-backend"
alias lunacredentialing="cd ~/projects/luna-project/therapist-credentialing-backend"
alias lunadashboard="cd ~/projects/luna-project/clinical-dashboard-backend"

# Git
alias gst="git status"
alias ga="git add"
alias gaA="git add -A"
alias gcm="git commit"
alias gcma="git commit -am"
alias gdf="git diff"

# Git Branching
alias gb="git branch"
alias gbd="git branch -d"
alias gcob="git checkout -b"
alias gco="git checkout"
alias gmrg="git merge"
alias grbs="git rebase"

# Git Remotes
alias gps="git push"
alias gpl="git pull"
alias gf="git fetch"
alias grso="git remote show origin"
alias gpruneorigin="git remote prune origin"

# Hub
alias hpr="hub pull-request -c"

# Git Log
alias glg="git log --graph"
alias glol="git log -n"
alias glone="git log --oneline"

# Vagrant
alias vst="vagrant status"
alias vup="vagrant up"
alias vssh="vagrant ssh"
alias vupssh="vagrant up && vagrant ssh"
alias vhalt="vagrant halt"
alias vdestroyf="vagrant destroy -f"

# Ruby on Rails
alias be="bundle exec"
alias bi="bundle install"
alias rdbm="rails db:migrate"
alias rserv="rails server"
alias rcon="rails console"

# Containers
alias alpha_container="~/projects/scripts/x_task.sh -p alpha"
alias beta_container="~/projects/scripts/x_task.sh -p beta"

alias alpha_patient_forms_container="~/projects/scripts/x_task.sh -p alpha -s patient-self-report sh"
alias alpha_credentialing_container="~/projects/scripts/x_task.sh -p alpha -s therapist-signup sh"
alias alpha_provider_portal_container="~/projects/scripts/x_task.sh -p alpha -s provider-portal sh"

alias alpha_edge_container="~/projects/scripts/x_task.sh -p alpha -s edge sh"
alias alpha_marketplace_container="~/projects/scripts/x_task.sh -p alpha -s marketplace sh"

# Logs
alias alphalogspatients="~/projects/scripts/x_logs.sh alpha service patient-self-report 15m"
alias alphalogstherapists="~/projects/scripts/x_logs.sh alpha service therapist-signup 15m"

# Generate Dumps
alias alpha_patient_forms_generate_dump="~/projects/scripts/x_make_dev_db.sh alpha patient-self-report"
alias alpha_edge_generate_dump="~/projects/scripts/x_make_dev_db.sh alpha edge"
alias alpha_marketplace_generate_dump="~/projects/scripts/x_make_dev_db.sh alpha marketplace"

# Get dump access
alias alpha_dump_creds="~/projects/scripts/x_rds_dev_creds.sh alpha"

# AWS stuff
alias s3ls="aws s3 ls"
alias s3cp="aws s3 cp"

alias awsloginalpha="aws sso login --profile alpha"
alias awsloginbeta="aws sso login --profile beta"
alias awsloginomega="aws sso login --profile omega"
