#!/usr/bin/env bash

## Work Projects Navigation
#
alias luna_credentialing="cd ~/projects/luna-project/therapist-credentialing-backend"
alias luna_dashboard="cd ~/projects/luna-project/provider-portal"
alias luna_backend="cd ~/projects/luna-project/backend"
alias luna_marketplace="cd ~/projects/luna-project/marketplace"

## Generate Dumps
#
alias alpha_therapist_signup_generate_dump="luna db create-dev-db --environment alpha --database therapist-signup"
alias alpha_provider_portal_generate_dump="luna db create-dev-db --environment alpha --database provider-portal"
alias alpha_edge_generate_dump="luna db create-dev-db --environment alpha --database edge"
alias alpha_marketplace_generate_dump="luna db create-dev-db --environment alpha --database marketplace"

## AWS
#
alias awsloginalpha="aws sso login --profile alpha"
alias awsloginomega="aws sso login --profile omega"

## Zellij
#
alias zetwopanes="zellij --layout two_panes"
alias zeluna="zellij --layout luna"
alias zedap="zellij --layout devaspros"

## Pods
#
alias alpha_therapist_signup_pods="kubectl get po -n therapist-credentialing-backend"
alias alpha_provider_portal_pods="kubectl get po -n physician-portal"
alias alpha_edge_pods="kubectl get po -n backend"
alias alpha_marketplace_pods="kubectl get po -n marketplace"
